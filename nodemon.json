{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "src/**/*.spec.ts"
  ],
  "verbose": true,
  "execMap": {
    "ts": "ts-node"
  },
  "watch": [
    "src/**/*"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "ts,js,json",
  "exec": "serverless offline start"
}