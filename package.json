{
  "name": "zth-serverless-nodejs-typescript-journey",
  "version": "0.1.0",
  "description": "Repositório dedicado a explorar a arquitetura serverless através de um percurso com NodeJS e TypeScript.",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "prepare": "husky install",
    "commit": "git add . && git-cz",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:changed": "jest -o",
    "test:inBand": "jest --runInBand",
    "test:changedSince": "jest --changedSince=main",
    "test:clearCache": "jest --clearCache"
  },
  "keywords": [
    "template",
    "node",
    "typescript"
  ],
  "author": "Thiago Andrade Silva",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.5",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.1",
    "@types/uuid": "^9.0.1",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}