{
  "name": "zth-serverless-nodejs-typescript-journey",
  "version": "0.1.0",
  "description": "Repositório dedicado a explorar a arquitetura serverless através de um percurso com NodeJS e TypeScript.",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "prepare": "husky install",
    "commit": "git add . && git-cz",
    "release": "standard-version",
    "jest": "jest --silent",
    "test": "npm run jest",
    "test:watch": "npm run jest -- --watch",
    "test:coverage": "npm run jest -- --coverage",
    "test:changed": "npm run jest -- -o",
    "test:inBand": "npm run jest -- --runInBand",
    "test:changedSince": "npm run jest -- --changedSince=main",
    "test:clearCache": "jest --clearCache",
    "sls:offline": "serverless offline start"
  },
  "keywords": [
    "template",
    "node",
    "typescript"
  ],
  "author": "Thiago Andrade Silva",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.5",
    "@serverless/typescript": "^3.30.1",
    "@types/aws-lambda": "^8.10.117",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.1",
    "@types/uuid": "^9.0.1",
    "aws-sdk-client-mock": "^2.1.1",
    "aws-sdk-mock": "^5.8.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "serverless": "^3.32.2",
    "serverless-esbuild": "^1.45.1",
    "serverless-offline": "^12.0.4",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.348.0",
    "@aws-sdk/lib-dynamodb": "^3.348.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1394.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.0",
    "uuid": "^9.0.0"
  }
}